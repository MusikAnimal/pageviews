{
  "env": {
    "es6": true,
    "browser": true
  },
  "rules": {
    "no-alert": 0,
    "no-array-constructor": 0,
    "no-bitwise": 0,
    "no-caller": 0,
    "no-catch-shadow": 0,
    "no-comma-dangle": 0,
    "no-cond-assign": 0,
    "no-console": 0,
    "no-constant-condition": 0,
    "no-continue": 0,
    "no-control-regex": 0,
    "no-debugger": 2,
    "no-delete-var": 0,
    "no-div-regex": 0,
    "no-dupe-keys": 2,
    "no-dupe-args": 0,
    "no-duplicate-case": 0,
    "no-else-return": 0,
    "no-empty": 2,
    "no-empty-class": 0,
    "no-empty-character-class": 0,
    "no-empty-label": 0,
    "no-eq-null": 0,
    "no-eval": 2,
    "no-ex-assign": 0,
    "no-extend-native": 0,
    "no-extra-bind": 1,
    "no-extra-boolean-cast": 0,
    "no-extra-parens": 0,
    "no-extra-semi": 2,
    "no-extra-strict": 0,
    "no-fallthrough": 0,
    "no-floating-decimal": 0,
    "no-func-assign": 0,
    "no-implied-eval": 0,
    "no-inline-comments": 0,
    "no-inner-declarations": [0, "functions"],
    "no-invalid-regexp": 0,
    "no-irregular-whitespace": 0,
    "no-iterator": 0,
    "no-label-var": 0,
    "no-labels": 0,
    "no-lone-blocks": 0,
    "no-lonely-if": 1,
    "no-loop-func": 0,
    "no-mixed-requires": [0, false],
    "no-mixed-spaces-and-tabs": [0, false],
    "linebreak-style": [2, "unix"],
    "no-multi-spaces": 0,
    "no-multi-str": 0,
    "no-multiple-empty-lines": [0, {"max": 0}],
    "no-native-reassign": 0,
    "no-negated-in-lhs": 0,
    "no-nested-ternary": 2,
    "no-new": 0,
    "no-new-func": 0,
    "no-new-object": 0,
    "no-new-require": 0,
    "no-new-wrappers": 0,
    "no-obj-calls": 0,
    "no-octal": 0,
    "no-octal-escape": 0,
    "no-param-reassign": 0,
    "no-path-concat": 0,
    "no-plusplus": 0,
    "no-process-env": 0,
    "no-process-exit": 0,
    "no-proto": 0,
    "no-redeclare": 2,
    "no-regex-spaces": 0,
    "no-reserved-keys": 0,
    "no-restricted-modules": 0,
    "no-return-assign": 0,
    "no-script-url": 1,
    "no-self-compare": 2,
    "no-sequences": 0,
    "no-shadow": 0,
    "no-shadow-restricted-names": 0,
    "no-space-before-semi": 0,
    "no-spaced-func": 0,
    "no-sparse-arrays": 0,
    "no-sync": 0,
    "no-ternary": 0,
    "no-trailing-spaces": 0,
    "no-this-before-super": 0,
    "no-throw-literal": 0,
    "no-undef": 0,
    "no-undef-init": 0,
    "no-undefined": 0,
    "no-unexpected-multiline": 0,
    "no-underscore-dangle": 0,
    "no-unneeded-ternary": 0,
    "no-unreachable": 2,
    "no-unused-expressions": 0,
    "no-unused-vars": [2, {"vars": "local", "args": "none"}],
    "no-use-before-define": 0,
    "no-void": 0,
    "no-var": 0,
    "prefer-const": 0,
    "no-warning-comments": [0, { "terms": ["todo", "fixme", "xxx"], "location": "start" }],
    "no-with": 0,
    "no-wrap-func": 0,

    "array-bracket-spacing": [0, "never"],
    "accessor-pairs": 0,
    "block-scoped-var": 0,
    "brace-style": [0, "1tbs"],
    "callback-return": 0,
    "camelcase": 0,
    "comma-dangle": [0, "never"],
    "comma-spacing": 0,
    "comma-style": 0,
    "complexity": [0, 11],
    "computed-property-spacing": [0, "never"],
    "consistent-return": 0,
    "consistent-this": [0, "that"],
    "constructor-super": 0,
    "curly": [0, "all"],
    "default-case": 0,
    "dot-location": 0,
    "dot-notation": [0, { "allowKeywords": true }],
    "eol-last": 0,
    "eqeqeq": 2,
    "func-names": 0,
    "func-style": [0, "declaration"],
    "generator-star": 0,
    "generator-star-spacing": 0,
    "global-strict": [0, "never"],
    "guard-for-in": 0,
    "handle-callback-err": 0,
    "indent": [2, 2],
    "init-declarations": 0,
    "key-spacing": [0, { "beforeColon": false, "afterColon": true }],
    "keyword-spacing": 2,
    "lines-around-comment": 0,
    "max-depth": [0, 4],
    "max-len": [0, 80, 4],
    "max-nested-callbacks": [0, 0],
    "max-params": [0, 3],
    "max-statements": [0, 10],
    "new-cap": 0,
    "new-parens": 0,
    "newline-after-var": 0,
    "object-curly-spacing": [0, "never"],
    "object-shorthand": 0,
    "one-var": 0,
    "operator-assignment": [0, "always"],
    "operator-linebreak": 0,
    "padded-blocks": 0,
    "quote-props": 0,
    "quotes": [1, "single", "avoid-escape"],
    "radix": 0,
    "require-yield": 0,
    "semi": [2, "always"],
    "semi-spacing": [0, {"before": false, "after": true}],
    "sort-vars": 0,
    "space-after-function-name": [0, "never"],
    "space-before-blocks": [0, "always"],
    "space-before-function-paren": [0, "always"],
    "space-before-function-parentheses": [0, "always"],
    "space-in-brackets": [0, "never"],
    "space-in-parens": [0, "never"],
    "space-infix-ops": 0,
    "space-return-throw-case": 0,
    "space-unary-ops": [0, { "words": true, "nonwords": false }],
    "spaced-comment": 0,
    "spaced-line-comment": [0, "always"],
    "strict": 0,
    "use-isnan": 0,
    "valid-jsdoc": 1,
    "valid-typeof": 2,
    "vars-on-top": 0,
    "wrap-iife": 0,
    "wrap-regex": 0,
    "yoda": [0, "never"]
  }
}
